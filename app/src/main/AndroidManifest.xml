<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ash.customerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/OtherActivityMainPageTheme">
        <activity
            android:name=".LoginActivity"
            android:theme="@style/LoginPageTheme">

        </activity>
        <activity android:name=".OpenProfileActivity" />
        <activity android:name=".OpenNotificationActivity" />
        <activity android:name=".OpenOrdersActivity" />
        <activity android:name=".OpenCartActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/MainActivitySplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>